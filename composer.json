{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "bensampo/laravel-enum": "^6.11",
        "beyondcode/laravel-websockets": "^1.14",
        "ckfinder/ckfinder-laravel-package": "^5.0",
        "egulias/email-validator": "^4.0",
        "genealabs/laravel-model-caching": "11.0.1",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/agent": "2.6",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/scout": "^9.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "3.1.58",
        "matchish/laravel-scout-elasticsearch": "^7.9",
        "mews/purifier": "3.4",
        "mindscms/laravelshoppingcart": "^2.1",
        "nwidart/laravel-modules": "5.0",
        "predis/predis": "^2.2",
        "protonemedia/laravel-xss-protection": "1.5",
        "spatie/laravel-permission": "6.9",
        "spatie/laravel-responsecache": "^7.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "timwassenburg/laravel-service-generator": "^1.0"
    },
    "autoload": {   
        "files" : [
            "app/Helper/helpers.php" 
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/",
            "CKSource\\" : "app/Overrides/CKSource/"
        }
       
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    }
}
